class Solution:
    def multiply(self, num1: str, num2: str) -> str:
         if num1=='0' or num2=='0':
             return '0'
         result= f"{int (num1)*int(num2)}"
         
         if 1 <= len(num1) <= 200 and 1 <= len(num2) <= 200 :
            return result.lstrip("0")
         else :
            return 0
or simply
class Solution:
    def multiply(self, num1: str, num2: str) -> str:
        return f"{int(num1)*int(num2)}"
